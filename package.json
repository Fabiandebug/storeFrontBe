{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "npm run build && nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "migrate-test": "db-migrate --env test up && db-migrate up",
    "demigrate-test": "db-migrate --env test down",
    "test": "npm run build && set ENV=test&& db-migrate --env test up && db-migrate up && jasmine-ts --random=false && db-migrate --env test down -c 4",
    "jasmine": "jasmine",
    "jasmine-ts": "jasmine-ts",
    "build": "npx tsc",
    "tsc": "tsc",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "lint": "eslint \"src/**/*.ts\""
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
