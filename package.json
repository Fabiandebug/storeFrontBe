    "@types/body-parser": "^1.19.2",
    "@types/eslint": "^8.4.6",
    "@types/express": "^4.17.15",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.37.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
